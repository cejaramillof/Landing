!!! 5
%html
  %head
    %title Dashboard
    = favicon_link_tag 'favicon.ico'
    = stylesheet_link_tag 'dashboard', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'dashboard', 'data-turbolinks-track' => true
    = csrf_meta_tags
    %link{rel:'stylesheet', href:'https://fonts.googleapis.com/css?family=Noto+Sans:400,700', type:'text/css'}
    %meta{name:"viewport", content:"width=device-width, initial-scale=1"}
  %body
    = custom_bootstrap_flash
    = render "partials/dashnavbar"
    = yield